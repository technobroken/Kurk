[application]

config/name="Krurk Revolution"
run/main_scene="res://Scenes/PlayScene.tscn"
config/icon="res://icon.png"

[autoload]

Asserts="*res://Tools/Asserts.gd"
NodeTools="*res://Tools/NodeTools/NodeTools.gd"
BulletContainer="*res://Tools/BulletContainer.gd"

[display]

window/size/width=288
window/size/height=162
window/size/test_width=1408
window/size/test_height=792
window/stretch/mode="2d"
window/stretch/aspect="keep"

